{
  "extends": "airbnb",
  "rules": {
    "max-len": [
      1,
      120,
      2,
      {
        "ignoreComments": true
      }
    ],
    "quotes": [1, "single"],
    "indent": [1, 2],
    "linebreak-style": [1, "unix"],
    "semi": [1, "always"],
    "quote-props": [
      1,
      "consistent-as-needed"
    ],
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "radix": 0,
    "default-case": 0,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "comma-spacing": 1,
    "arrow-parens": [1, "always"],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-extra-parens": 0,
    "brace-style": 1,
    "object-curly-spacing": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-trailing-spaces": 1,
    "no-unused-vars": 1,
    "key-spacing": 1,
    "padded-blocks": 1,
    "comma-dangle": [1, "never"],
    "space-before-function-paren": 0,
    "object-shorthand": 0,
    "space-infix-ops": 1
  }
}